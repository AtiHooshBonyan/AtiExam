@model IEnumerable<UserResponse>
<h1>User Responses</h1>
<table class="table">
    <thead>
        <tr>
            <th>Session</th>
            <th>Question</th>
            <th>Answer</th>
            <th>Is Correct</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.SessionId</td>
                <td>@item.QuestionId</td>
                <td>@item.AnswerId</td>
                <td>@item.IsCorrect</td>
                <td>
                    <a asp-action="Details" asp-route-id="@item.ResponseId">Details</a> |
                    <a asp-action="Edit" asp-route-id="@item.ResponseId">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@item.ResponseId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>